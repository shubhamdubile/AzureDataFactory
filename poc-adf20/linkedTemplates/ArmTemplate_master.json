{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "poc-adf20"
		},
		"LS_Destination_Shubham_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_Destination_Shubham'"
		},
		"LS_Source_Shubham_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_Source_Shubham'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adfstorageaccount.dfs.core.windows.net/"
		},
		"LS_Destination_Shubham_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adfstorageaccount.dfs.core.windows.net/"
		},
		"LS_managed_identity_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adfstorageaccount.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "poc-adf20_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_Destination_Shubham_accountKey": {
						"value": "[parameters('LS_Destination_Shubham_accountKey')]"
					},
					"LS_Source_Shubham_connectionString": {
						"value": "[parameters('LS_Source_Shubham_connectionString')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"LS_Destination_Shubham_properties_typeProperties_url": {
						"value": "[parameters('LS_Destination_Shubham_properties_typeProperties_url')]"
					},
					"LS_managed_identity_properties_typeProperties_url": {
						"value": "[parameters('LS_managed_identity_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}